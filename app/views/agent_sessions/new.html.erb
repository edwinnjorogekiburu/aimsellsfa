<% provide(:title, "Sign in") %>
<h1>Agents Sign in</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for(:agent_session, url: agent_sessions_path) do |f| %>

			<%=f.label :email %>
			<%=f.text_field :email %>

			<%=f.label :password %>
			<%=f.password_field :password %>

			<%=f.submit "Sign in", class: "btn btn-large btn-primary" %>
		<p>
			<%= link_to "Forgot your Password/Reset Password", '#' %>
		</p>
		<% end %> 
	</div>
</div>