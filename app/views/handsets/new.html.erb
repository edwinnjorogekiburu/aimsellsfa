<% provide(:title,"Add handset") %>
<div class="row">
  <aside class="span4">
    <section>
     <%= render "shared/employee_side_menu" %>  
    </section>
  </aside>
<div class="span8"> 
	<p><%= link_to "All handsets" , handsets_path %></p>
<h2>Add Handset</h2>
<%= form_for(@handset) do |f| %>
      <%= render partial: 'shared/error_messages' , :locals => { :object => @handset }%>
      <%= render partial: "handset_fields" , :locals => {:f => f } %>
      <%= f.submit "Add handset", class: "btn btn-large btn-primary" %>
<% end %>
</div>
</div>
