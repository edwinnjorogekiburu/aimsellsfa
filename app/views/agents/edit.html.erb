<% provide(:title,"Edit agent") %>
<div class="row">
  <aside class="span4">
    <section>
     <%= render "shared/employee_side_menu" %>  
    </section>
  </aside>
<div class="span8"> 
<p><%= link_to "All Agents" , agents_path %></p>
<h2>Edit agent</h2>
<%= form_for(@agent) do |f| %>
      <%= render partial: 'shared/error_messages' , :locals => { :object => @agent }%>
      <%= render partial: "agent_fields" , :locals => {:f => f } %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<% end %>
</div>
</div>
